{
  "name": "autoupdate-action",
  "version": "1.8.0",
  "description": "A GitHub Action that auto-updates PRs with changes from their base branch.",
  "main": "src/autoupdater.ts",
  "repository": "https://github.com/alxndrbauer/autoupdate",
  "author": "Chin Godawita <chin.godawita@me.com>",
  "license": "MIT",
  "bin": {
    "autoupdate-action": "bin/cli.js"
  },
  "scripts": {
    "build": "tsc bin/cli.ts --outDir dist",
    "lint": "eslint . && prettier --list-different bin/*.ts src/*.ts test/*.ts",
    "lint:fix": "eslint --fix .",
  "test": "jest --runInBand",
    "test:watch": "jest --watchAll"
  ,"test:unit:new": "vitest run --dir new-tests/unit",
  "test:unit:new:watch": "vitest --dir new-tests/unit",
  "test:integration:new": "vitest run --dir new-tests/integration",
  "test:coverage:new": "vitest run --coverage --dir new-tests",
  "test:type": "tsd"
  },
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/github": "^6.0.1",
    "@octokit/types": "^14.1.0",
    "@octokit/webhooks": "^14.1.3",
    "@octokit/webhooks-types": "^7.6.1",
    "@types/node": "^24.3.0",
    "ts-patch": "^3.3.0",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@types/jest": "29.5.0",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "29.5.0",
    "jest-ts-auto-mock": "^2.1.0",
    "nock": "^14.0.10",
    "prettier": "^3.6.2",
    "ts-auto-mock": "^3.7.4",
  "ts-jest": "^29.4.1",
  "vitest": "^1.6.0",
  "@vitest/coverage-v8": "^1.6.0",
  "@types/node": "^24.3.0",
  "msw": "^2.3.0",
  "fast-check": "^3.17.0",
  "tsd": "^0.30.0"
  }
}
